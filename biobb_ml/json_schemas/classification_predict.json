{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://bioexcel.eu/biobb_ml/json_schemas/1.0/classification_predict",
    "name": "biobb_ml ClassificationPredict",
    "title": "Makes predictions from an input dataset and a given classification model.",
    "description": "Makes predictions from an input dataset (prodided either as a file or as a dictionary property) and a given classification model trained with DecisionTreeClassifier, KNeighborsClassifier, LogisticRegression, RandomForestClassifier, Support Vector Machine methods.",
    "type": "object",
    "info": {
        "wrapped_software": {
            "name": "scikit-learn",
            "version": ">=0.23.1",
            "license": "BSD 3-Clause"
        },
        "ontology": {
            "name": "EDAM",
            "schema": "http://edamontology.org/EDAM.owl"
        }
    },
    "required": [
        "input_model_path",
        "output_results_path"
    ],
    "properties": {
        "input_model_path": {
            "type": "string",
            "description": "Path to the input model",
            "filetype": "input",
            "sample": "https://github.com/bioexcel/biobb_ml/raw/master/biobb_ml/test/data/classification/model_classification_predict.pkl",
            "enum": [
                ".*\\.pkl$"
            ],
            "file_formats": [
                {
                    "extension": ".*\\.pkl$",
                    "description": "Path to the input model",
                    "edam": "format_3653"
                }
            ]
        },
        "input_dataset_path": {
            "type": "string",
            "description": "Path to the dataset to predict",
            "filetype": "input",
            "sample": "https://github.com/bioexcel/biobb_ml/raw/master/biobb_ml/test/data/classification/input_classification_predict.csv",
            "enum": [
                ".*\\.csv$"
            ],
            "file_formats": [
                {
                    "extension": ".*\\.csv$",
                    "description": "Path to the dataset to predict",
                    "edam": "format_3752"
                }
            ]
        },
        "output_results_path": {
            "type": "string",
            "description": "Path to the output results file",
            "filetype": "output",
            "sample": "https://github.com/bioexcel/biobb_ml/raw/master/biobb_ml/test/reference/classification/ref_output_classification_predict.csv",
            "enum": [
                ".*\\.csv$"
            ],
            "file_formats": [
                {
                    "extension": ".*\\.csv$",
                    "description": "Path to the output results file",
                    "edam": "format_3752"
                }
            ]
        },
        "properties": {
            "type": "object",
            "properties": {
                "predictions": {
                    "type": "array",
                    "default": null,
                    "wf_prop": false,
                    "description": "List of dictionaries with all values you want to predict targets. It will be taken into account only in case **input_dataset_path** is not provided. Format: [{ 'var1': 1.0, 'var2': 2.0 }, { 'var1': 4.0, 'var2': 2.7 }] TODO FOR NOT COLUMNS!!!!"
                },
                "remove_tmp": {
                    "type": "boolean",
                    "default": true,
                    "wf_prop": true,
                    "description": "Remove temporal files."
                },
                "restart": {
                    "type": "boolean",
                    "default": false,
                    "wf_prop": true,
                    "description": "Do not execute if output files exist."
                }
            }
        }
    },
    "additionalProperties": false
}