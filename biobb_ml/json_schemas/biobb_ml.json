{
    "_id": "biobb_ml",
    "desc": "Biobb_ml is the Biobb module collection to perform machine learning predictions.",
    "github": "https://github.com/bioexcel/biobb_ml",
    "readthedocs": "https://biobb-ml.readthedocs.io/en/latest/",
    "conda": "",
    "docker": "",
    "singularity": "",
    "version": "3.0.1",
    "tools" : [
        {
            "block" : "LinearRegression", 
            "tool" : "sklearn.linear_model.LinearRegression", 
            "desc" : "Trains and tests a given dataset and saves the model and scaler for a linear regression.",
            "exec" : "linear_regression",
            "docs": "https://biobb-ml.readthedocs.io/en/latest/regression.html#module-regression.linear_regression"
        },
        {
            "block" : "PolynomialRegression", 
            "tool" : "sklearn.linear_model.LinearRegression", 
            "desc" : "Trains and tests a given dataset and saves the model and scaler for a polynomial regression.",
            "exec" : "polynomial_regression",
            "docs": "https://biobb-ml.readthedocs.io/en/latest/regression.html#module-regression.polynomial_regression"
        },
        {
            "block" : "RandomForestRegressor", 
            "tool" : "sklearn.ensemble.RandomForestRegressor", 
            "desc" : "Trains and tests a given dataset and saves the model and scaler for a random forest regressor.",
